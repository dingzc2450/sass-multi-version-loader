{
  "name": "sass-multi-version-loader",
  "version": "0.0.6",
  "description": "Sass loader for webpack",
  "license": "MIT",
  "author": "Dz",
  "homepage": "https://github.com/dingzc2450/sass-multi-version-loader",
  "bugs": "https://github.com/dingzc2450/sass-multi-version-loader/issues",
  "main": "dist/loader.js",
  "engines": {
    "node": ">= 16 || >= 14 || >= 12"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "commitlint": "commitlint --from=master",
    "security": "npm audit --production",
    "lint:prettier": "prettier --cache --list-different .",
    "lint:js": "eslint --cache .",
    "lint:spelling": "cspell --cache --no-must-find-files --quiet \"**/*.*\"",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "test:only": "cross-env NODE_ENV=test jest",
    "fix:js": "npm run lint:js -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "npm-run-all -l fix:js fix:prettier",
    "test:watch": "npm run test:only -- --watch",
    "test:manual": "npm run build && webpack-dev-server test/manual/src/index.js --open --config test/manual/webpack.config.js",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage",
    "prepare": "husky && npm run build",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "sass": "^1.3.0",
    "webpack": "^2.x.x"
  },
  "peerDependenciesMeta": {
    "sass": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "async": "^3.2.5",
    "clone-deep": "^0.2.4",
    "loader-utils": "^1.0.1",
    "pify": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.1.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-jest": "^29.6.2",
    "bootstrap-sass": "^3.4.1",
    "bootstrap-v4": "npm:bootstrap@^4.5.3",
    "bootstrap-v5": "npm:bootstrap@^5.0.1",
    "cross-env": "^7.0.3",
    "cspell": "^8.6.0",
    "del": "^6.1.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.28.0",
    "foundation-sites": "^6.7.5",
    "husky": "^9.0.11",
    "jest": "^29.6.2",
    "jest-environment-node-single-context": "^29.1.0",
    "lint-staged": "^15.2.0",
    "material-components-web": "^9.0.0",
    "memfs": "^4.7.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.2",
    "sass": "^1.77.8",
    "semver": "^7.5.4",
    "should": "^13.2.3",
    "standard-version": "^9.3.1",
    "webpack": "^2.2.1"
  },
  "keywords": [
    "sass",
    "webpack",
    "loader"
  ]
}
